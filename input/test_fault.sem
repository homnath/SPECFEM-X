#pre information
preinfo: method='sem', ngllx=3, nglly=3, ngllz=3, nenode=8, ngnode=8, inp_path=./input', out_path=./outp./'

#mesh information
mesh: xfile='test_fault_coord_x', yfile='test_fault_coord_y', zfile='test_fault_coord_z', &
confile='test_fault_connectivity', idfile='test_fault_material_id'

#boundary conditions
bc: uxfile='test_fault_ssbcux', uyfile='test_fault_ssbcuy', uzfile='test_fault_ssbcuz'

#initial stress
#stress0: type=1, z0=10, s0=0
 
#traction
#traction: trfile='test_fault_trfile'

#material list
material: matfile='test_fault_material_list'

#control parameters
control: ksp_tol=1e-6, ksp_maxiter=2000, nl_tol=1e-4, nl_maxiter=50, ntstep=1, dt=50, ninc=1

#save data
save: disp=1
