#pre information
preinfo: nproc=96, method='sem', ngllx=3, nglly=3, ngllz=3, nenode=8, ngnode=8, inp_path=./input', out_path=./outp./'

#mesh information
mesh: xfile='okada_replication_tripling_coord_x', yfile='okada_replication_tripling_coord_y', zfile='okada_replication_tripling_coord_z', &
confile='okada_replication_tripling_connectivity', idfile='okada_replication_tripling_material_id', gfile='okada_replication_tripling_ghost'

#boundary conditions
bc: uxfile='okada_replication_tripling_ssbcux', uyfile='okada_replication_tripling_ssbcuy', uzfile='okada_replication_tripling_ssbcuz'

#initial stress
#stress0: type=1, z0=10, s0=0
 
#traction
#traction: trfile='okada_replication_tripling_trfile'

#material list
material: matfile='okada_replication_tripling_material_list'

#control parameters
control: ksp_tol=1e-6, ksp_maxiter=2000, nl_tol=1e-4, nl_maxiter=50, ntstep=1, dt=50, ninc=1

#save data
save: disp=1
