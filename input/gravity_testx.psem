#pre information
preinfo: nproc=16, method='sem', ngllx=3, nglly=3, ngllz=3, nenode=8, ngnode=8, &
inp_path='./input', out_path='./output_gravity/', &
pot_dof=1, disp_dof=1

#mesh information
mesh: xfile='gravity_testx_coord_x', yfile='gravity_testx_coord_y', zfile='gravity_testx_coord_z', &
confile='gravity_testx_connectivity', idfile='gravity_testx_material_id', gfile='gravity_testx_ghost'

#boundary conditions
bc: uxfile='gravity_testx_ssbcux', uyfile='gravity_testx_ssbcuy', uzfile='gravity_testx_ssbcuz', &
infbc=1,add_infmesh=1,infrfile='gravity_testx_infinite_surface_addinf', &
matidinf=3,zmaxinf=1,infquad='radau',pole0='user', &
coord=0.0000 0.0000 -225.0000,pole_type='point',rinf=2000.0,valinf=0

#initial stress
#stress0: type=1, z0=10, s0=0
 
#traction
#traction: trfile='nepal_topo_trfile'

#earthquake slip or CMT file. 0 = slip,1 = CMT, 2 = Finite fault, 3 = Slip with split node
eqsource: type=0, slipfile='gravity_testx_slip'

#material list
material: matfile='gravity_testx_material_list',density=1

#control parameters
control: ksp_tol=1e-6, ksp_maxiter=1000, nl_tol=1e-4, nl_maxiter=1

#save data
save: disp=1,gpot=1

#devel
devel: nondim=1
