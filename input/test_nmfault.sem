#pre information
preinfo: method='sem', ngllx=3, nglly=3, ngllz=3, nenode=8, ngnode=8, inp_path=./input', out_path=./outp./'

#mesh information
mesh: xfile='test_nmfault_coord_x', yfile='test_nmfault_coord_y', zfile='test_nmfault_coord_z', &
confile='test_nmfault_connectivity', idfile='test_nmfault_material_id'

#boundary conditions
bc: uxfile='test_nmfault_ssbcux', uyfile='test_nmfault_ssbcuy', uzfile='test_nmfault_ssbcuz'

#initial stress
#stress0: type=1, z0=10, s0=0
 
#traction
#traction: trfile='test_nmfault_trfile'

#material list
material: matfile='test_nmfault_material_list',ispart=0

#control parameters
control: ksp_tol=1e-6, ksp_maxiter=2000, nl_tol=1e-4, nl_maxiter=50, ntstep=1, dt=50, ninc=1

#save data
save: disp=1
